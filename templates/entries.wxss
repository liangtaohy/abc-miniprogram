.entry-list {
	display: flex;
	flex-direction: row;
	width: 100%;
	justify-content: space-around;
	flex-wrap: wrap;
}

.entry-card {
	width: 210rpx;
	height: 210rpx;
	box-sizing: border-box;
	border: 2rpx solid black;
	align-items: center;
	flex-direction: column;
	display: flex;
	padding: 6rpx;
	border-radius: 33rpx;
	margin-bottom: 14rpx;
}

.entry-img {
	width: 150rpx;
	height: 150rpx;
}

.entry-cover {
	width: 100%;
	height: 100%;
}